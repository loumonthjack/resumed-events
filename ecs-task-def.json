{
    "family": "resumed-event-task",
    "containerDefinitions": [
        {
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/resumed-events-service/containers",
                    "awslogs-region": "us-west-2",
                    "awslogs-create-group": "true",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "name": "resumed-events-service",
            "image": "412325484896.dkr.ecr.us-west-2.amazonaws.com/resumed-events:latest",
            "portMappings": [
                {
                    "name": "resumed-events-service-4000-tcp",
                    "containerPort": 4000,
                    "hostPort": 4000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [
                {
                    "value": "arn:aws:s3:::api.resumed.events/events.env",
                    "type": "s3"
                }
            ],
            "mountPoints": [],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::412325484896:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::412325484896:role/ecsTaskExecutionRole",
    "networkMode": "host",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "8192",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}